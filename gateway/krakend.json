{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "KrakenD - API Gateway",
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "endpoints": [
    {
      "endpoint": "/api/v1/gsearch/{query}",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/{query}",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "http://service-gsearch:8002/"
          ],
          "disable_host_sanitize": false
        }
      ],
      "extra_config": {
        "qos/ratelimit/router": {
          "max_rate": 5,
          "client_max_rate": 0,
          "strategy": "ip"
        }
      },
      "input_query_strings": []
    }
  ]
}